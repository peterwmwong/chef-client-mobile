@import 'animation'
@import 'box'
@import 'gradient'
@import 'pos'
@import 'triangle'

.ANDROID
  .App
    abspos 0px 0px -70px 0px
    padding-bottom 69px

.IOS
  .App
    abspos 0px 0px -60px 0px

.App
  abspos 0px 0px 0px 0px
  box vertical
  -webkit-tap-highlight-color rgba(0,0,0,0)
  z-index 1

  > .TitleBar
    abspos 0px 0px false 0px
    z-index 3
    
  > #content
    margin-top 43px
    position relative
    box-flex 1
    z-index 1
